{
    "name": "cortxcli_setup",
    "description": "CortxCLI Setup Command",
    "sub_commands": [
        {
            "name": "config",
            "description": "Config Cortx CLI tool",
            "need_confirmation": false,
            "permissions_tag": "update",
            "args": [
                {
                  "flag": "-a",
                  "dest": "Address",
                  "help": "CSM Agent Address",
                  "default": "127.0.0.1",
                  "type": "str",
                  "choices": [
                    "127.0.0.1"
                  ],
                  "nargs": "?"
                },
                {
                    "flag": "-f",
                    "action": "store_true",
                    "default": false,
                    "help": "force"
                },
                {
                    "flag": "--debug",
                    "action": "store_true",
                    "default": false,
                    "help": "config for dev mode"
                },
                {
                  "flag": "-c",
                  "dest": "Component",
                  "help": "Component name",
                  "default": "all",
                  "type": "str",
                  "choices": [
                    "backend",
                    "frontend",
                    "all"
                  ],
                  "nargs": "?"
                },
                {
                    "flag": "args",
                    "default": [],
                    "suppress_help": true,
                    "nargs": "?"
                }
            ],
            "comm": {
            "type":"CLI",
            "method": "cmd",
            "target": "",
            "version": "v1"
            }
        }
    ]
}
